{% for key, value in data.items() %}
  {%- if key == 'listen-address' -%}
    WPM_LISTEN_ADDR={{ value }}
  {%- endif %}
  {%- if key == 'interface' -%}
    WPM_INTERFACE={{ value }}
  {%- endif %}
  {%- if key == 'api-token' -%}
    WPM_SECRET={{ value }}
  {%- endif %}
  {%- if key == 'dns' -%}
    WPM_DNS={{ value }}
  {%- endif %}
  {%- if key == 'endpoint' -%}
    WPM_ENDPOINT={{ value }}
  {%- endif %}
  {%- if key == 'name' -%}
    WPM_SERVER_ID={{ value }}
  {%- endif %}
{% endfor %}